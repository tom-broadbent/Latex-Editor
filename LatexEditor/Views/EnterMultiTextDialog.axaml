<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="using:LatexEditor.ViewModels"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="LatexEditor.Views.EnterMultiTextDialog"
        x:DataType="vm:EnterMultiTextDialogViewModel">
  
  <Grid x:Name="MainGrid" ColumnDefinitions="*, *">
    <Button x:Name="OkButton" Grid.Column="0" HorizontalAlignment="Right" Width="64" HorizontalContentAlignment="Center" HotKey="Enter">Ok</Button>
    <Button x:Name="CancelButton" Grid.Column="1" Width="64" HorizontalContentAlignment="Center" HotKey="Escape">Cancel</Button>
  </Grid>
</Window>
